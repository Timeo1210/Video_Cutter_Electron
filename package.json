{
  "name": "video-cutter",
  "version": "1.0.0",
  "description": "Win and darwin app for fast4video",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "devKill": "taskkill /f /im python3.exe",
    "devPythonStart": "python3 pythonScripts/client.py",
    "package-win": "electron-packager . fast4videos --overwrite --ignore='pythonScripts$' --ignore='pythonScripts' --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Fast4Videos\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Timeo1210/Video_Cutter_Electron.git"
  },
  "author": "Boulhol Timeo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Timeo1210/Video_Cutter_Electron/issues"
  },
  "homepage": "https://github.com/Timeo1210/Video_Cutter_Electron#readme",
  "devDependencies": {
    "electron": "^8.0.1",
    "electron-packager": "^14.2.1",
    "electron-winstaller": "^4.0.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "socket.io": "^2.3.0",
    "strftime": "^0.10.0",
    "zeromq": "^6.0.0-beta.6"
  }
}
